<h1>bookings new</h1>

<div class="container">
    <div class="row">
      <div class="col-6 m-auto">
        <div class="flat-form">
          <h1>Book your Friend</h1>
          <div class="mt-3">
            <%= simple_form_for([@friend_service, @booking]) do |f| %>
              <%= f.input :start_date, as: :date, html5: true %>
              <%= f.input :end_date, as: :date, html5: true %>
              <%= f.submit %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
</div>
