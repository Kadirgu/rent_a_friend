
<%= simple_form_for(friend_service) do |f| %>
  <%= f.input :photo, as: :file %>
  <%= f.input :title%>
  <%= f.input :description%>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :age %>
  <%= f.input :phone_number %>
  <%= f.input :email %>
  <%= f.input :gender %>
  <%= f.input :category %>
  <%= f.input :availability, as: :date, html5: true %>
  <%= f.input :interests %>
  <%= f.submit %>
<% end %>
